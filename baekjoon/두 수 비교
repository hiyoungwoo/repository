
import java.util.*;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int input = s.nextInt();
		int input2 = s.nextInt();
		if ( input < input2) {
			System.out.println("<");
		}
		else if ( input == input2) {
			System.out.println("==");
		}
		else if ( input > input2) {
			System.out.println(">");
		}

	}

}
