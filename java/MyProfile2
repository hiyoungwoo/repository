package week03;
import java.util.*;

public class MyProfile2 {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub;
    Scanner s = new Scanner (System.in);
   
    
    System.out.println("당신의 이름을 입력하세요.");
    String a = s.next();
    System.out.println("내 이름은 "+ a );  
    
    System.out.println("당신의 나이를 입력하세요.");
    int b = s.nextInt();
    System.out.println("내 나이는 "+ b );
    
    System.out.println("당신의 생년월일을 입력하세요.");
    long  c = s.nextLong();
    System.out.println("내 생년월일 8자리는 "+ c );
    
    System.out.println("당신의 키를 입력하세요.");
    double d = s.nextDouble();
    System.out.println("내 키는 "+ d);

    System.out.println("당신의 취미를 입력하세요.");
    String e = s.next();
    System.out.println("취미는  "+ e );
    
    
 
    System.out.println("당신의 입력한 내용은 이름은" + a +"나이는"+ b +"생년월일 8자리는"+ c + "키는" + d + 
 	"취미는"+ e + "입니다");
    
    
	}

}
